#include <iostream>
using namespace std;

int main()
{
    
char base;
cout<<"Enter the base of the number: ";
cin>>base;

switch(base){

case 'D'
case 'd'

case 'H'
case 'h'

case 'O'
case 'o'

case 'b'
case 'B'

default
break;
}


return 0;
}


